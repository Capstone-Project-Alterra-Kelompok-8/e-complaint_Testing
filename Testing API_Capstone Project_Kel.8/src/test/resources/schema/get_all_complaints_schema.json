{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Welcome2",
  "definitions": {
    "Welcome2": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "status": {
          "type": "boolean"
        },
        "message": {
          "type": "string"
        },
        "metadata": {
          "$ref": "#/definitions/Metadata"
        },
        "data": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Datum"
          }
        }
      },
      "required": [
        "data",
        "message",
        "metadata",
        "status"
      ],
      "title": "Welcome2"
    },
    "Datum": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string"
        },
        "user": {
          "$ref": "#/definitions/User"
        },
        "category": {
          "$ref": "#/definitions/Category"
        },
        "regency": {
          "$ref": "#/definitions/Regency"
        },
        "address": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "status": {
          "type": "string"
        },
        "type": {
          "$ref": "#/definitions/Type"
        },
        "files": {
          "anyOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/File"
              }
            },
            {
              "type": "null"
            }
          ]
        },
        "date": {
          "$ref": "#/definitions/Date"
        },
        "total_likes": {
          "type": "integer"
        },
        "updated_at": {
          "type": "string"
        }
      },
      "required": [
        "address",
        "category",
        "date",
        "description",
        "files",
        "id",
        "regency",
        "status",
        "total_likes",
        "type",
        "updated_at",
        "user"
      ],
      "title": "Datum"
    },
    "Category": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        }
      },
      "required": [
        "description",
        "id",
        "name"
      ],
      "title": "Category"
    },
    "File": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "integer"
        },
        "complaint_id": {
          "type": "string"
        },
        "path": {
          "type": "string"
        }
      },
      "required": [
        "complaint_id",
        "id",
        "path"
      ],
      "title": "File"
    },
    "Regency": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string",
          "format": "integer"
        },
        "name": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "name"
      ],
      "title": "Regency"
    },
    "User": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "$ref": "#/definitions/Name"
        },
        "email": {
          "$ref": "#/definitions/Email"
        },
        "telephone_number": {
          "type": "string"
        },
        "profile_photo": {
          "$ref": "#/definitions/ProfilePhoto"
        }
      },
      "required": [
        "email",
        "id",
        "name",
        "profile_photo",
        "telephone_number"
      ],
      "title": "User"
    },
    "Metadata": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "total_data": {
          "type": "integer"
        },
        "Pagination": {
          "$ref": "#/definitions/Pagination"
        }
      },
      "required": [
        "Pagination",
        "total_data"
      ],
      "title": "Metadata"
    },
    "Pagination": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "total_data_per_page": {
          "type": "integer"
        },
        "first_page": {
          "type": "integer"
        },
        "last_page": {
          "type": "integer"
        },
        "current_page": {
          "type": "integer"
        },
        "next_page": {
          "type": "integer"
        },
        "prev_page": {
          "type": "integer"
        }
