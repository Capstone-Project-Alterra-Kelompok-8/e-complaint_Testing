    },
    "Type": {
      "type": "string",
      "enum": [
        "public",
        "private"
      ],
      "title": "Type"
    },
    "Email": {
      "type": "string",
      "enum": [
        "mannperii16@gmail.com",
        "user3@gmail.com",
        "user1@gmail.com",
        "user2@gmail.com"
      ],
      "title": "Email"
    },
    "Name": {
      "type": "string",
      "enum": [
        "testing_my_updated",
        "User 3",
        "User 1",
        "User 2"
      ],
      "title": "Name"
    },
    "ProfilePhoto": {
      "type": "string",
      "enum": [
        "profile-photos/default.jpg"
      ],
      "title": "ProfilePhoto"
    }
  }
}
